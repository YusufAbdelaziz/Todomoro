{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["react"],
  "parser": "babel-eslint",
  "rules": {
    "prefer-destructuring": "warn",
    "react/jsx-uses-react": "warn",
    "arrow-parens": "off",
    "no-unused-vars": "warn"
  },
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2021,
    "ecmaFeatures": {
      "destructuring": true,
      "modules": true,
      "jsx": true
    }
  }
}
